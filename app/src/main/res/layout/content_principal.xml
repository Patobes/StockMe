<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="stockme.stockme.Principal"
    tools:showIn="@layout/app_bar_principal">

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButtonListas"
        android:clickable="true"
        android:src="@mipmap/listas"
        android:background="#00ffffff"
        android:layout_marginTop="32dp"
        android:layout_alignParentStart="true"
        android:onClick="openListas"/>

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButtonStock"
        android:src="@mipmap/stock"
        android:background="#00ffffff"
        android:clickable="true"
        android:layout_alignTop="@+id/imageButtonListas"
        android:layout_alignParentEnd="true" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButtonMercados"
        android:src="@mipmap/listas"
        android:background="#00ffffff"
        android:layout_below="@+id/imageButtonListas"
        android:layout_alignParentStart="true"
        android:layout_marginTop="96dp"
        android:clickable="true" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButtonOpciones"
        android:src="@mipmap/listas"
        android:background="#00ffffff"
        android:layout_alignTop="@+id/imageButtonMercados"
        android:layout_alignParentEnd="true"
        android:clickable="true" />
</RelativeLayout>
